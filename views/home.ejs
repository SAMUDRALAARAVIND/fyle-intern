    <p id="limit" style="display:none"><%= ejsData.limit %></p>
    <p id="offset" style="display:none"><%= ejsData.offset %></p>
    <br><br><br><br>
    <div class="container" ng-app="app" ng-controller="ctrl">
        <div class="row bg-primary" style="padding:100px 20px;border-radius:1%">
          <div class="col-sm-3">
          <select class="btn btn-success" id="stateSelect">
            <option value="TELANGANA">TELANGANA</option>
            <option value="ANDHRA PRADESH">ANDHRA PRADESH</option>
            <option value="KARNATAKA">KARNATAKA</option>
            <option value="MAHARASHTRA">MAHARASHTRA</option>
            <option value="KERALA">KERALA</option>
          </select>
        </div>
        <div class="col-sm-3">  </div>
        <div class="col-sm-6">
          <input type="text" id="search" class="form-control" placeholder="Search By Bank Names" ng-keyup="func()">

            <p ng-repeat="x in serverResponse"  name="liSearchResult" class="searchResult" onclick="func(this.innerHTML)">{{x.bank_name}}</p>

        </div>
